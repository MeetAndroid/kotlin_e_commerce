<+>plugins {\r\n    id 'com.android.application'\r\n    id 'org.jetbrains.kotlin.android'\r\n    id 'androidx.navigation.safeargs.kotlin'\r\n    id 'kotlin-android'\r\n    id 'kotlin-kapt'\r\n    id 'dagger.hilt.android.plugin'\r\n    id 'com.google.dagger.hilt.android'\r\n    id 'com.google.gms.google-services'\r\n}\r\n\r\nandroid {\r\n\r\n    defaultConfig {\r\n        applicationId \"com.spec.spec_ecommerce\"\r\n        minSdk 24\r\n        targetSdk 32\r\n        compileSdkVersion 33\r\n        versionCode 1\r\n        versionName \"1.0\"\r\n        vectorDrawables.useSupportLibrary = true\r\n        testInstrumentationRunner \"androidx.test.runner.AndroidJUnitRunner\"\r\n    }\r\n\r\n    buildTypes {\r\n\r\n        debug {\r\n            debuggable true\r\n            buildConfigField 'String', \"BASE_URL\", '\"http://10.37.57.105:8080/apis/v1/\"'\r\n        }\r\n\r\n        release {\r\n            minifyEnabled false\r\n            proguardFiles getDefaultProguardFile('proguard-android-optimize.txt'), 'proguard-rules.pro'\r\n        }\r\n    }\r\n    compileOptions {\r\n        sourceCompatibility JavaVersion.VERSION_1_8\r\n        targetCompatibility JavaVersion.VERSION_1_8\r\n    }\r\n    kotlinOptions {\r\n        jvmTarget = '1.8'\r\n    }\r\n\r\n    buildFeatures {\r\n        viewBinding = true\r\n    }\r\n    kotlinOptions {\r\n        freeCompilerArgs += [\r\n                \"-Xjvm-default=all\",\r\n        ]\r\n    }\r\n\r\n}\r\n\r\n// Allow references to generated code\r\nkapt {\r\n    correctErrorTypes = true\r\n}\r\n\r\ndependencies {\r\n\r\n    def splashApiVersion = \"1.0.0\"\r\n    def retrofitVersion = \"2.6.2\"\r\n    def loggingInterceptorVersion = \"4.10.0\"\r\n    def roomVersion = \"2.4.3\"\r\n    def lifeCycleVersion = \"2.5.1\"\r\n    def coroutinesVersion = \"1.6.1\"\r\n    def navigationVersion = \"2.5.1\"\r\n    def glideVersion = \"4.13.2\"\r\n    def dataStoreVersion = \"1.0.0\"\r\n    def hiltVersion = \"2.43.2\"\r\n    def brManagerVersion = \"1.1.0\"\r\n    def indicator = \"1.2.1\"\r\n    def facebook_sdk_version = \"14.1.0\"\r\n    def scalars_version = \"2.5.0\"\r\n    def lottie_version = \"5.2.0\"\r\n    def shimmer_effect_version = \"0.5.0\"\r\n    def kotlin_serialization = \"1.4.0-RC\"\r\n    def gmail_version = \"20.3.0\"\r\n    def firebase_version = \"24.3.0\"\r\n    def google_map_version = \"18.1.0\"\r\n    def fused_location_version = \"20.0.0\"\r\n    def place_api_version = \"2.6.0\"\r\n\r\n    // Splash API to support Android 12 and higher devices\r\n    implementation \"androidx.core:core-splashscreen:$splashApiVersion\"\r\n\r\n    // Retrofit dependencies for Network Call\r\n    implementation \"com.squareup.retrofit2:retrofit:$retrofitVersion\"\r\n    implementation \"com.squareup.retrofit2:converter-gson:$retrofitVersion\"\r\n    implementation \"com.squareup.okhttp3:logging-interceptor:$loggingInterceptorVersion\"\r\n\r\n    // Room Database for offline support\r\n    implementation \"androidx.room:room-runtime:$roomVersion \"\r\n    kapt \"androidx.room:room-compiler:$roomVersion\"\r\n    // Kotlin Extensions and Coroutines support for Room\r\n    implementation \"androidx.room:room-ktx:$roomVersion\"\r\n\r\n    // Kotlin Extensions and Coroutines support for Room\r\n    implementation \"androidx.room:room-ktx:$roomVersion\"\r\n\r\n    // ViewModel Scope\r\n    implementation \"androidx.lifecycle:lifecycle-viewmodel-ktx:$lifeCycleVersion\"\r\n    implementation \"androidx.lifecycle:lifecycle-viewmodel-savedstate:$lifeCycleVersion\"\r\n    // Lifecycle Scope\r\n    implementation \"androidx.lifecycle:lifecycle-runtime-ktx:$lifeCycleVersion\"\r\n    // Live Data\r\n    implementation \"androidx.lifecycle:lifecycle-livedata-ktx:$lifeCycleVersion\"\r\n\r\n    // Coroutines for asynchronous task\r\n    implementation \"org.jetbrains.kotlinx:kotlinx-coroutines-core:$coroutinesVersion\"\r\n    implementation \"org.jetbrains.kotlinx:kotlinx-coroutines-android:$coroutinesVersion\"\r\n\r\n    // Navigation\r\n    implementation \"androidx.navigation:navigation-fragment-ktx:$navigationVersion\"\r\n    implementation \"androidx.navigation:navigation-ui-ktx:$navigationVersion\"\r\n\r\n    // Glide for Image Loading\r\n    implementation \"com.github.bumptech.glide:glide:$glideVersion\"\r\n    kapt \"com.github.bumptech.glide:compiler:$glideVersion\"\r\n\r\n    // For store shared preference data\r\n    implementation \"androidx.datastore:datastore-preferences:$dataStoreVersion\"\r\n\r\n    // Hilt dependency injection\r\n    implementation \"com.google.dagger:hilt-android:$hiltVersion\"\r\n    kapt \"com.google.dagger:hilt-android-compiler:$hiltVersion\"\r\n\r\n    implementation \"androidx.localbroadcastmanager:localbroadcastmanager:$brManagerVersion\"\r\n\r\n    //on boarding indicator\r\n    implementation \"com.github.zhpanvip:viewpagerindicator:$indicator\"\r\n\r\n    //FB SDK\r\n    implementation \"com.facebook.android:facebook-android-sdk:$facebook_sdk_version\"\r\n\r\n    // Scalars for converting Request Model class to JSON String\r\n    implementation \"com.squareup.retrofit2:converter-scalars:$scalars_version\"\r\n\r\n    // Lottie Animation Lib\r\n    implementation \"com.airbnb.android:lottie:$lottie_version\"\r\n\r\n    //otp view\r\n    implementation 'com.github.pdrozz:pinview:1.0.2'\r\n\r\n    //Shimmer Effect\r\n    implementation \"com.facebook.shimmer:shimmer:$shimmer_effect_version\"\r\n\r\n    // Kotlin Serialization\r\n    implementation \"org.jetbrains.kotlinx:kotlinx-serialization-json:$kotlin_serialization\"\r\n\r\n    // Shimmer Effect\r\n    implementation \"com.github.skydoves:androidveil:1.1.2\"\r\n\r\n    //gmail login\r\n    implementation \"com.google.android.gms:play-services-auth:$gmail_version\"\r\n\r\n    //Google Map\r\n    implementation \"com.google.android.gms:play-services-maps:$google_map_version\"\r\n\r\n    // FusedLocation\r\n    implementation \"com.google.android.gms:play-services-location:$fused_location_version\"\r\n\r\n    // Firebase\r\n    implementation \"com.google.firebase:firebase-firestore-ktx:$firebase_version\"\r\n\r\n    // Place API\r\n    implementation \"com.google.android.libraries.places:places:$place_api_version\"\r\n\r\n    implementation 'androidx.core:core-ktx:1.8.0'\r\n    implementation 'androidx.appcompat:appcompat:1.5.0'\r\n    implementation 'com.google.android.material:material:1.6.1'\r\n    implementation 'com.google.android.material:material:1.6.1'\r\n    implementation 'androidx.constraintlayout:constraintlayout:2.1.4'\r\n    testImplementation 'junit:junit:4.13.2'\r\n    androidTestImplementation 'androidx.test.ext:junit:1.1.3'\r\n    androidTestImplementation 'androidx.test.espresso:espresso-core:3.4.0'\r\n\r\n}
